# Frontend Configuration
REACT_APP_API_URL=http://localhost:9999

# Backend Configuration
PORT=9999
NODE_ENV=development

# OpenAI Configuration
# Note: API key is now managed through the frontend UI
# No need to set OPENAI_API_KEY in the backend

# OpenAI Model Configuration
# Available models:
# - gpt-4o-mini (recommended for this application)
# - gpt-3.5-turbo
# - gpt-4
OPENAI_MODEL=gpt-4o-mini

# MongoDB URI (if running locally)
MONGODB_URI=mongodb://localhost:27017/voicechatbot 